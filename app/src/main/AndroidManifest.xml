<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.develop.playpadel.playpadel">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Inicio" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".ValoracionPartidoActivity" />
        <activity android:name=".MisPartidosActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".BuscarParticipanteActivity" />
        <activity android:name=".BuscarPartidoActivity" />
        <activity android:name=".CrearPartidoActivity" />
        <activity android:name=".MiPerfilActivity"></activity>
    </application>

</manifest>